#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/metro/src/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/metro/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/metro/src/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules/metro/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/metro@0.80.9_bufferutil@4.0.8_utf-8-validate@6.0.4/node_modules:/Users/mickarundel/React/loopDaWoop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../metro/src/cli.js" "$@"
else
  exec node  "$basedir/../metro/src/cli.js" "$@"
fi
